	    <TableRow
	        android:id="@+id/rowIdentificacion"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_below="@+id/rowSolicitud"
	        android:gravity="left" >

		    <Button
		        android:id="@+id/btnIdentificacion"
		        android:layout_width="fill_parent"
		        android:layout_height="fill_parent"
		        android:background="@drawable/stylebtns_formulario"
		        android:drawableLeft="@drawable/camera20x20"
		        android:drawablePadding="10dp"
		        android:drawableRight="@drawable/go_icon20x20"
		        android:gravity="left|center_vertical"
		        android:paddingLeft="10dp"
		        android:paddingRight="5dp"
		        android:text="@string/btnIdentificacion"
		        android:textAlignment="gravity"
		        android:textSize="18sp" />

		</TableRow>

	    <TableRow
	        android:id="@+id/rowSolicitud"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginTop="20dp"
	        android:gravity="left" >

		    <Button
		        android:id="@+id/btnSolicitud"
		        android:layout_width="fill_parent"
		        android:layout_height="fill_parent"
		        android:background="@drawable/stylebtns_formulario"
		        android:drawableLeft="@drawable/camera20x20"
		        android:drawablePadding="10dp"
		        android:drawableRight="@drawable/go_icon20x20"
		        android:gravity="left|center_vertical"
		        android:paddingLeft="10dp"
		        android:paddingRight="5dp"
		        android:text="@string/btnSolicitud"
		        android:textAlignment="gravity"
		        android:textSize="18sp" />

