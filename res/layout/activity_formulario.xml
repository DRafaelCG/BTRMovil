<?xml version="1.0" encoding="utf-8"?>
<ScrollView 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
	android:background="@drawable/background"    
    tools:context=".Formulario">

	<LinearLayout
	    android:id="@+id/LLayoutFormulario"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:layout_marginTop="0dp"
	    android:orientation="vertical"
	    android:scrollbarAlwaysDrawVerticalTrack="true" >

    <RelativeLayout
        android:id="@+id/rLayoutAnexar"
        android:layout_width="fill_parent"
        android:layout_height="35dp"
        android:background="#000000"
        android:visibility="invisible" >

        <TextView
            android:id="@+id/tvAnexar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginBottom="2dp"
            android:layout_marginTop="2dp"
            android:text="@string/tvAnexar"
            android:textColor="#ffffff"
            android:textSize="14sp" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rLayoutTextSolicitante"
        android:layout_width="fill_parent"
        android:layout_height="35dp"
        android:background="@drawable/background">

		<TextView
		    android:id="@+id/tVDatosCliente"
		    android:layout_width="150dp"
		    android:layout_height="20dp"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="10dp"
		    android:layout_marginTop="15dp"
		    android:text="@string/tVDatosCliente"
		    android:textColor="#a80000"
		    android:textSize="18sp"
		    android:textStyle="bold" />

    </RelativeLayout>
		<!-- <View
		    android:layout_width="fill_parent"
			android:layout_height="1dp"
			android:background="@android:color/darker_gray"/>-->

		<RelativeLayout
		    android:id="@+id/rLayoutDatosGenerales"
		    android:layout_width="fill_parent"
		    android:layout_height="400dp"
		    android:background="#FFFFFF"
		    android:orientation="vertical"
		    android:scrollbarAlwaysDrawVerticalTrack="true"
		    android:scrollbars="vertical"
		    android:visibility="visible" >

			<!-- 
			<TextView
			    android:id="@+id/tVDatosCliente"
			    android:layout_width="150dp"
			    android:layout_height="20dp"
			    android:layout_marginLeft="10dp"
			    android:layout_marginTop="15dp"
			    android:text="@string/tVDatosCliente"
			    android:textSize="18sp"
			    android:textStyle="bold" 
			    android:textColor="#FF1300"/>-->

		    <TextView
		        android:id="@+id/tVpNombre"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_marginLeft="10dp"
		        android:layout_marginTop="20dp"
		        android:text="@string/tVNombre1"
		        android:textSize="14sp" />

	    	<EditText
	    	    android:id="@+id/eTpNombre"
	    	    android:layout_width="wrap_content"
	    	    android:layout_height="wrap_content"
	    	    android:layout_alignBaseline="@+id/tVpNombre"
	    	    android:layout_alignBottom="@+id/tVpNombre"
	    	    android:layout_alignParentRight="true"
	    	    android:layout_marginLeft="15dp"
	    	    android:layout_marginRight="15dp"
	    	    android:layout_toRightOf="@+id/tVpNombre"
	    	    android:ems="10"
	    	    android:inputType="textCapSentences" 
	    	    android:background="@drawable/edittext_rounded_corners">
		        
				<requestFocus/>

			</EditText>
			<!-- android:hint="@string/eTpNombre"-->
			<TextView
			    android:id="@+id/tVsNombre"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_alignLeft="@+id/tVpNombre"
			    android:layout_below="@+id/tVpNombre"
			    android:layout_marginTop="20dp"
			    android:text="@string/tVNombre2"
			    android:textSize="14sp" />

			<EditText
			    android:id="@+id/eTsNombre"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_alignBaseline="@+id/tVsNombre"
			    android:layout_alignBottom="@+id/tVsNombre"
			    android:layout_alignLeft="@+id/eTpNombre"
			    android:layout_alignRight="@+id/eTpNombre"
			    android:background="@drawable/edittext_rounded_corners"
			    android:ems="10"
			    android:inputType="textCapSentences"
			    android:paddingLeft="6dp" />

			<!-- android:hint="@string/eTsNombre"-->
			<TextView
			    android:id="@+id/tVapPaterno"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_alignLeft="@+id/tVsNombre"
			    android:layout_below="@+id/tVsNombre"
			    android:layout_marginTop="20dp"
			    android:text="@string/tVApPaterno"
			    android:textSize="14sp" />

			<EditText
			    android:id="@+id/eTapPaterno"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_alignBaseline="@+id/tVapPaterno"
			    android:layout_alignBottom="@+id/tVapPaterno"
			    android:layout_alignRight="@+id/eTsNombre"
			    android:layout_alignLeft="@+id/eTsNombre"
			    android:background="@drawable/edittext_rounded_corners"
			    android:ems="10"
			    android:inputType="textCapSentences"
			    android:paddingLeft="6dp"/>
			<!-- android:hint="@string/eTapPaterno"-->
			<TextView 
			    android:id="@+id/tVapMaterno"
				android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_marginTop="20dp"
		        android:layout_below="@+id/tVapPaterno"
		        android:layout_alignLeft="@+id/tVapPaterno"
		        android:text="@string/tVApMaterno"/>

			<EditText
			    android:id="@+id/eTapMaterno"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_alignBaseline="@+id/tVapMaterno"
			    android:layout_alignBottom="@+id/tVapMaterno"
			    android:layout_alignLeft="@+id/eTapPaterno"
			    android:layout_alignRight="@+id/eTapPaterno"
			    android:background="@drawable/edittext_rounded_corners"
			    android:ems="10"
			    android:inputType="textCapSentences"
			    android:paddingLeft="6dp"/>
			<!-- android:hint="@string/eTapMaterno"-->

			<TextView
			    android:id="@+id/tVFechaNacimiento"
			    android:layout_width="80dp"
			    android:layout_height="wrap_content"
			    android:layout_alignLeft="@+id/tVapMaterno"
			    android:layout_below="@+id/tVapMaterno"
			    android:layout_marginTop="20dp"
			    android:maxLines="2"
			    android:text="@string/tVFechaNacimiento" />

			<EditText
			    android:id="@+id/eTFechaNacimiento"
			    android:layout_width="120dp"
			    android:layout_height="wrap_content"
			    android:layout_alignBottom="@+id/tVFechaNacimiento"
			    android:layout_alignTop="@+id/tVFechaNacimiento"
			    android:layout_centerHorizontal="true"
			    android:background="@drawable/edittext_rounded_corners"
			    android:editable="true"
			    android:ems="10"
			    android:hint="01/01/1900"
			    android:inputType="date"
			    android:onClick="SeleccionaFecha"
			    android:paddingLeft="10dp" 
			    android:cursorVisible="false"
			    android:focusable="false"
			    android:focusableInTouchMode="false"/>


			<TextView 
			    android:id="@+id/tVRfc"
				android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_marginTop="20dp"
		        android:layout_below="@+id/tVFechaNacimiento"
		        android:layout_alignLeft="@+id/tVFechaNacimiento"
		        android:text="@string/tVRfc"/>

			<EditText
			    android:id="@+id/eTRfc"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_alignBaseline="@+id/tVRfc"
			    android:layout_alignBottom="@+id/tVRfc"
			    android:layout_alignLeft="@+id/eTapMaterno"
			    android:layout_alignRight="@+id/eTapMaterno"
			    android:background="@drawable/edittext_rounded_corners"
			    android:ems="10"
			    android:inputType="textCapCharacters"
			    android:maxLength="13"
			    android:paddingLeft="6dp"/>
			<!-- android:hint="@string/eTRfc"-->
			<TextView 
			    android:id="@+id/tVTelCelular"
				android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_marginTop="20dp"
		        android:layout_below="@+id/tVRfc"
		        android:layout_alignLeft="@+id/tVRfc"
		        android:text="@string/tVTelCelular"/>

			<EditText
			    android:id="@+id/eTTelCcelular"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_alignBaseline="@+id/tVTelCelular"
			    android:layout_alignBottom="@+id/tVTelCelular"
			    android:layout_alignLeft="@+id/eTapMaterno"
			    android:layout_alignRight="@+id/eTapMaterno"
			    android:background="@drawable/edittext_rounded_corners"
			    android:ems="10"
			    android:inputType="phone"
			    android:maxLength="10"
			    android:paddingLeft="6dp" />

			<TextView 
			    android:id="@+id/tVCorreo"
				android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_marginTop="20dp"
		        android:layout_below="@+id/tVTelCelular"
		        android:layout_alignLeft="@+id/tVTelCelular"
		        android:text="@string/tVCorreo"/>

			<EditText
			    android:id="@+id/eTCorreo"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_alignBaseline="@+id/tVCorreo"
			    android:layout_alignBottom="@+id/tVCorreo"
			    android:layout_alignLeft="@+id/eTapMaterno"
			    android:layout_alignRight="@+id/eTapMaterno"
			    android:background="@drawable/edittext_rounded_corners"
			    android:ems="10"
			    android:inputType="textEmailAddress"
			    android:paddingLeft="6dp"/>
			<TextView 
			    android:id="@+id/tVProducto"
				android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_marginTop="30dp"
		        android:layout_below="@+id/tVCorreo"
		        android:layout_alignLeft="@+id/tVCorreo"
		        android:text="@string/tVProducto"/>
			<Spinner
			    android:id="@+id/sProductos"
			    android:layout_width="190dp"
			    android:layout_height="30dp"
			    android:layout_alignBottom="@+id/tVProducto"
			    android:layout_alignLeft="@id/eTCorreo"
			    android:layout_alignRight="@+id/eTCorreo"
			    android:layout_toRightOf="@+id/tVProducto"
			    android:background="@drawable/edittext_rounded_corners"
			    android:gravity="left"
			    android:scrollbarStyle="insideOverlay"
			    android:scrollbars="horizontal|vertical"
			    android:textAlignment="viewStart"
			    android:textSize="16sp" />

			

		</RelativeLayout>

		<RelativeLayout
		    android:id="@+id/rLayoutIncidencias"
		    android:layout_width="fill_parent"
		    android:layout_height="285dp"
		    android:orientation="vertical"
		    android:visibility="invisible" 
		    android:background="#FFFFFF">

		    <RelativeLayout
		        android:id="@+id/relativeLayout1"
		        android:layout_width="fill_parent"
		        android:layout_height="match_parent"
		        android:layout_below="@+id/tVIncidencias"
		        android:layout_marginBottom="10dp"
		        android:layout_marginLeft="10dp"
		        android:layout_marginRight="10dp"
		        android:layout_marginTop="10dp"
		        android:background="#FFFFFF" >

		        <TextView
		            android:id="@+id/tVIncid"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_alignParentTop="true"
		            android:layout_marginLeft="20dp"
		            android:layout_marginTop="30dp"
		            android:text="@string/tVIncid"
		            android:textSize="16sp" />
		    </RelativeLayout>

		    <TextView
		        android:id="@+id/tVIncidencias"
		        android:layout_width="150dp"
		        android:layout_height="20dp"
		        android:layout_alignLeft="@+id/relativeLayout1"
		        android:layout_alignParentTop="true"
		        android:layout_marginTop="22dp"
		        android:text="@string/tVIncidencias"
		        android:textSize="18sp"
		        android:textStyle="bold" />

		</RelativeLayout>
		<View
		    android:layout_width="fill_parent"
			android:layout_height="1dp"
			android:background="@android:color/darker_gray"/>
		<RelativeLayout
        	android:id="@+id/rLayoutTextDocumentacion"
        	android:layout_width="fill_parent"
        	android:layout_height="35dp"
        	android:background="@drawable/background">

			<TextView
			    android:id="@+id/tVDoctos"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:layout_centerVertical="true"
			    android:layout_marginLeft="10dp"
			    android:layout_marginTop="15dp"
			    android:text="@string/tVDoctos"
			    android:textColor="#a80000"
			    android:textSize="18sp" />

    </RelativeLayout>

		<RelativeLayout
		    android:id="@+id/rLayoutDoctos"
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:background="#FFFFFF" >

	    <TableRow
