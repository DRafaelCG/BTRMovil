		    <Button
		        android:id="@+id/btnCompDom"
		        android:layout_width="fill_parent"
		        android:layout_height="fill_parent"
		        android:background="@drawable/stylebtns_formulario"
		        android:drawableLeft="@drawable/camera20x20"
		        android:drawablePadding="10dp"
		        android:drawableRight="@drawable/go_icon20x20"
		        android:gravity="left|center_vertical"
		        android:paddingLeft="10dp"
		        android:paddingRight="5dp"
		        android:text="@string/btnCompDom"
		        android:textAlignment="gravity"
		        android:textSize="18sp" />

		</TableRow>
	    <TableRow
	        android:id="@+id/rowCompIng"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
